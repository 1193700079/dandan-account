<nav
  class="nav-instance"
  showIcons="{{['back']}}"
></nav>
<view class="target">
  <view class="target-header">
    <view class="target-name">{{targetInfo.targetData.name}}</view>
    <view class="menu">...</view>
  </view>
  <view class="target-content flex">
    <view class="target-item bg-white">
      <view class="target-item-title">账单总数</view>
      <view class="target-item-num">{{targetInfo.billList.length}}</view>
    </view>
    <view class="target-item bg-red color-white">
      <view class="target-item-title">时间进度</view>
      <view class="target-item-num">{{progress.percentage}}</view>
    </view>
    <view class="target-item bg-pink color-white">
      <view class="target-item-title">错失</view>
      <view class="target-item-num">123</view>
    </view>
    <view class="target-item bg-white">
      <view class="target-item-title">账单总数</view>
      <view class="target-item-num">123</view>
    </view>

    <view class="target-item-big bg-purple color-white">
      <view class="target-item-title">进度</view>
      <view class="chart-content">
        <canvas
          canvas-id="linechart"
          id="linechart-month"
          class="charts"
          style="width: {{screenWidth * 0.84}}px;"
        ></canvas>
      </view>
    </view>
  </view>
</view>
